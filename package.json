    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
